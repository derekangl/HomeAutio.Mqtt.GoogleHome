{
  "google/home/dimmableLight": {
    "id": "google/home/dimmableLight",
    "type": "action.devices.types.LIGHT",
    "willReportState": false,
    "roomHint": "Some Room",
    "name": {
      "defaultNames": [],
      "name": "dimmable light",
      "nicknames": []
    },
    "deviceInfo": {
      "manufacturer": "Some Company",
      "model": "Some Model",
      "hwVersion": "1.0",
      "swVersion": "1.0"
    },
    "traits": [
      {
        "trait": "action.devices.traits.OnOff",
        "commands": { "action.devices.commands.OnOff": { "on": "google/home/dimmableLight/set" } },
        "state": {
          "on": {
            "topic": "google/home/dimmableLight",
            "valueMap": [
              {
                "mqtt": "on",
                "type": "value",
                "google": "true"
              },
              {
                "mqtt": "off",
                "type": "value",
                "google": "false"
              },
              {
                "mqtt": "0",
                "type": "value",
                "google": "false"
              },
              {
                "mqttMin": 1.0,
                "mqttMax": 100.0,
                "type": "range",
                "google": "true"
              }
            ]
          }
        }
      },
      {
        "trait": "action.devices.traits.Brightness",
        "commands": { "action.devices.commands.BrightnessAbsolute": { "brightness": "google/home/dimmableLight/set" } },
        "state": {
          "brightness": {
            "topic": "google/home/dimmableLight",
            "valueMap": [
              {
                "mqtt": "on",
                "type": "value",
                "google": "100"
              },
              {
                "mqtt": "off",
                "type": "value",
                "google": "0"
              }
            ]
          }
        }
      }
    ]
  },
